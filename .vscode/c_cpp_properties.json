{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/components/cwhttpd/include",
                "${workspaceFolder}/components/frogfs/include",
                "${workspaceFolder}/components/frogfs/third-party/heatshrink"
            ],
            "defines": [
                "__ESP_FILE__=NULL",
                "VSCODE=1",
                "UNIX=1",
                "CONFIG_ESPFS_USE_HEATSHRINK=y"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++14",
            "intelliSenseMode": "gcc-x64"
        },
        {
            "name": "ESP8266",
            "includePath": [
                "${workspaceFolder}/components/cwhttpd/include",
                "${workspaceFolder}/components/frogfs/include",
                "${workspaceFolder}/components/frogfs/third-party/heatshrink",
                "${ESP8266_RTOS_SDK}/components/**"
            ],
            "defines": [
                "__ESP_FILE__=NULL",
                "VSCODE=1",
                "FREERTOS=1",
                "CONFIG_IDF_TARGET_ESP8266=y",
                "CONFIG_CWHTTPD_MBEDTLS=y",
                "CONFIG_FROGFS_USE_HEATSHRINK=y"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++14",
            "intelliSenseMode": "gcc-x64"
        },
        {
            "name": "ESP32",
            "includePath": [
                "${workspaceFolder}/components/cwhttpd/include",
                "${workspaceFolder}/components/frogfs/include",
                "${workspaceFolder}/components/frogfs/third-party/heatshrink",
                "${env:IDF_PATH}/components/**"
            ],
            "defines": [
                "__ESP_FILE__=NULL",
                "VSCODE=1",
                "ESP_PLATFORM=1",
                "FREERTOS=1",
                "CONFIG_IDF_TARGET_ESP32=y",
                "CONFIG_ENABLE_FLASH_MMAP=y",
                "CONFIG_CWHTTPD_MBEDTLS=y",
                "CONFIG_FROGFS_USE_HEATSHRINK=y"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++14",
            "intelliSenseMode": "gcc-x64"
        },
        {
            "name": "auto",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c17",
            "cppStandard": "gnu++14",
            "intelliSenseMode": "linux-gcc-x64",
            "compileCommands": "build/compile_commands.json"
        }
    ],
    "version": 4
}